{
    "name": "byross/boc-macau-payment",
    "description": "",
    "keywords": [
        "byross",
        "boc-macau-payment"
    ],
    "homepage": "https://github.com/byross/boc-macau-payment",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ken",
            "email": "cowsea2012@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.36.2",
        "illuminate/support": "^8.0",
        "ext-openssl": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Byross\\BOCPayment\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Byross\\BOCPayment\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Byross\\BOCPayment\\Providers\\BOCPaymentServiceProvider"
            ],
            "aliases": {
                "LighthouseMediaLibrary": "Byross\\BOCPayment\\Facades\\BOCPayment"
            }
        }
    }
}
